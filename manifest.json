{
  "manifest_version": 3,
  "name": "Youtube Thumbnail Remover",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["dist/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}
